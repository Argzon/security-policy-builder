{
  "name": "@jupiterone/security-policy-builder",
  "version": "3.2.0",
  "description": "Configurable compilation of policies, standards, and procedures documentation.",
  "author": "JupiterOne <hello@jupiterone.io>",
  "keywords": [
    "security",
    "documentation",
    "compiler"
  ],
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "bin": {
    "psp": "bin/psp"
  },
  "scripts": {
    "clean": "rm -rf -- *.log *.zip",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "build": "babel ./lib --out-dir ./dist",
    "prebuild": "yarn test",
    "zip": "make",
    "dockerzip": "make dockerzip",
    "mkdocs": "make mkdocs",
    "pretest": "eslint .",
    "test": "nyc ava",
    "lcov": "nyc --reporter=html ava; open $(git rev-parse --show-toplevel)/coverage/index.html",
    "local-publish": "export $(grep -v '^#' .env | xargs); ./bin/psp publish -n -c ./config.json -t ./templates -a $J1_ACCOUNT_ID -k $J1_API_TOKEN"
  },
  "files": [
    "bin",
    "commands",
    "dist",
    "lib",
    "static"
  ],
  "dependencies": {
    "@jupiterone/jupiterone-client-nodejs": "^0.21.1",
    "@jupiterone/security-policy-templates": "^2.8.0",
    "chalk": "^2.3.1",
    "commander": "^4.0.0",
    "file-set": "^2.0.0",
    "fs-extra": "^5.0.0",
    "inquirer": "^5.1.0",
    "joi": "^13.1.2",
    "moment": "^2.22.2",
    "mustache": "^2.3.0",
    "node-fetch": "^2.6.0",
    "p-all": "^2.1.0",
    "p-map": "^1.2.0",
    "p-throttle": "^3.1.0",
    "pluralize": "^7.0.0",
    "progress": "^2.0.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "showdown": "^1.9.1",
    "strip-ansi": "^4.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nock": "^9.1.9",
    "nyc": "^11.4.1",
    "prettier": "^2.0.5"
  },
  "ava": {
    "files": [
      "test/unit/*.js",
      "test/integration/*.js"
    ],
    "sources": [
      "!test/fixtures/*"
    ]
  },
  "nyc": {
    "exclude": [
      "**/questions/**/*.js",
      "**/questions.js",
      "test/**"
    ]
  }
}
